}

key O {
    label:                              'O'
    base:                               'o'
    shift, capslock:                    'O'
#    numlock:                            '6'
    ctrl, alt, meta:                    none
}

key P {
    label:                              'P'
    base:                               'p'
    shift, capslock:                    'P'
